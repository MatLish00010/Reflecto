# App environment: development, production, etc.
NODE_ENV=development

# Sentry DSN for error tracking
SENTRY_DSN=

# Supabase public URL and anon key (client-side)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Supabase URL and anon key (server-side, if different)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# OpenAI API key (keep secret)
OPENAI_API_KEY=

# 32-byte base64 string for AES-256-GCM note encryption
# Generate: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
NOTE_SECRET_KEY=

# Redis URL for rate limiting (optional for development)
REDIS_URL=
